#include <io.h>
#include <stdio.h>
#include <stdint.h>
#include "system.h"
// Memory map
#define SW_REG (*(volatile uint32_t*)PIO_SW_BASE)
#define SDRAM_REG (*(volatile uint32_t*)SDRAM_BASE)
#define LED_REG (*(volatile uint32_t*)PIO_LED_BASE)
int main()
{
  int data;

  while(1)
  {
	printf("Input a number : (0 -> 255)\n");
	scanf("%d", &data);

  }
  return 0;
}
