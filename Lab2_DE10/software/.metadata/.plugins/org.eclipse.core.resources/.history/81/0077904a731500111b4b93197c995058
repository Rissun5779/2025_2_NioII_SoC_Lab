// INC
#include <unistd.h>
#include <stdint.h>
#include <io.h>
#include "system.h"
// Memory mapped
#define PIO_LED_REG *(volatile uint32_t PIO_LED_BASE)
#define PIO_SW_REG  *(volatile uint32_t PIO_SW_BASE)
// Variable
// MAIN
int main()
{
  char sw_input;

  while(1)
  {
    sw_input = PIO_SW_REG;
  }
  return 0;
}
