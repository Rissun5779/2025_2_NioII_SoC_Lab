// INC
#include <unistd.h>
#include <stdint.h>
#include <io.h>
#include "system.h"
// Memory mapped
#define PIO_LED_REG (*(volatile uint16_t *)PIO_LED_BASE)
#define PIO_SW_REG (*(volatile uint16_t *)PIO_SW_BASE)
// Variable
// MAIN
int main()
{
  uint16_t INPUT_SW = PIO_SW_REG;

  while (1){
    PIO_LED_REG = PIO_SW_REG;
  }
  return 0;
}
