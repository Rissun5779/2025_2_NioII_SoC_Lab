// INC
#include <unistd.h>
#include <stdint.h>
#include <io.h>
#include "system.h"
// Memory mapped
#define PIO_LED_REG (*(volatile uint32_t *)PIO_LED_BASE)
#define PIO_SW_REG (*(volatile uint32_t *)PIO_SW_BASE)
// Variable
// MAIN
int main()
{
  int GATE = 60;

  while (1){
     PIO_LED_REG = (PIO_SW_REG>=GATE)?
    		 	 	 	 0x11110000:
    		 	 	 	 0x00001111;
  }
  return 0;
}
