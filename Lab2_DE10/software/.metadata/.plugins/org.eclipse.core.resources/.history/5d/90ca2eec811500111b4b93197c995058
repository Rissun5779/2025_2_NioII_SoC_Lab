// INC
#include <unistd.h>
#include <stdint.h>
#include <io.h>
#include "system.h"
// Memory mapped
#define PIO_LED_REG (*(volatile uint32_t *)PIO_LED_BASE)
#define PIO_SW_REG (*(volatile uint32_t *)PIO_SW_BASE)
// Variable
// MAIN
int main()
{
  uint8_t count=0;

  while (1)
  {
     PIO_LED_REG = PIO_SW_REG;
  }
  return 0;
}
